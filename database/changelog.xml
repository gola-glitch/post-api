<?xml version="1.0" encoding="UTF-8"?>
<changelog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="http://www.liquigraph.org/schema/1.0/liquigraph.xsd">
    <changeset id="create-draft-id-unique-constraint" author="Hariharan">
        <query>CREATE CONSTRAINT draft_id_unique ON (draft:Draft) ASSERT draft.draftId IS UNIQUE</query>
    </changeset>
    <changeset id="create-user-id-unique-constraint" author="Hariharan">
        <query>CREATE CONSTRAINT person_id_unique ON (person:Person) ASSERT person.userId IS UNIQUE</query>
    </changeset>
    <changeset id="create-unique-name-on-interest" author="Hariharan">
        <query>CREATE CONSTRAINT interest_name_unique ON (interest:Interest) ASSERT interest.name IS UNIQUE</query>
    </changeset>
    <changeset id="create-unique-name-on-category" author="Hariharan">
        <query>CREATE CONSTRAINT category_name_unique ON (category:Category) ASSERT category.name IS UNIQUE</query>
    </changeset>
    <changeset id="create-unique-post" author="Hariharan">
        <query>CREATE CONSTRAINT post_unique ON (post:Post) ASSERT (post.puid) IS NODE KEY</query>
    </changeset>
    <changeset id="create-post-publish-audit-property-constraint" author="Hariharan">
        <query>CREATE CONSTRAINT post_publish_audit_property_constraint IF NOT EXISTS ON ()-[audit:PUBLISHED]-() ASSERT EXISTS (audit.createdAt)</query>
    </changeset>
