        <query>CREATE CONSTRAINT unique_comment IF NOT EXISTS ON ()-[commentObj:COMMENTED]-() ASSERT EXISTS (commentObj.comment)</query>
        <query>CREATE CONSTRAINT unique_comment_audit IF NOT EXISTS ON ()-[commentObj:COMMENTED]-() ASSERT EXISTS (commentObj.createdAt)</query>
